{
  "name": "@firestitch/example",
  "version": "0.1.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/Firestitch/fs-example"
  },
  "author": {
    "name": "Firestitch",
    "email": "admin@firestitch.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Firestitch/fs-example/issues"
  },
  "main": "package/index.js",
  "typings": "package/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean:build": "rimraf build",
    "clean:demo": "rimraf demo",
    "clean:package": "rimraf package",
    "copy:build": "cpx -v \"src/**/*\" \"build\"",
    "copy:styles": "cpx -v \"build/**/*.css\" \"package\"",
    "copy:templates": "cpx -v \"build/**/*.html\" \"package\"",
    "package": "npm-run-all -s clean:build clean:package copy:build build:sass package:replace-scss package:aot build:package copy:styles copy:templates package:minify clean:build",
    "package:aot": "ngc -p tsconfig.aot.json",
    "package:replace-scss": "node ./tools/replace-scss.js",
    "package:dev": "webpack --config=tools/webpack.dev.js --progress --profile --watch",
    "package:minify": "uglifyjs package/index.js --source-map --compress --mangle --screw-ie8 --output package/index.min.js",
    "build:package": "webpack --config=tools/webpack.package.js --progress --profile",
    "demo": "npm run clean:demo && webpack --config=tools/webpack.demo.js --progress --profile",
    "build:sass": "node-sass -r build/ -o build/ || true",
    "serve": "webpack-dev-server --config=tools/webpack.playground.js --progress --profile --watch",
    "lint": "tslint 'src/**/*.ts' -p ./tslint.json"
  },
  "dependencies": {
    "@angular/material": "^5.0.4",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20",
    "highlight.js": "^9.9.0"
  },
  "devDependencies": {
    "@angular/animations": "^5.2.3",
    "@angular/cdk": "^5.0.4",
    "@angular/cli": "^1.6.7",
    "@angular/common": "^5.2.3",
    "@angular/compiler": "^5.2.3",
    "@angular/compiler-cli": "^5.2.3",
    "@angular/core": "^5.2.3",
    "@angular/forms": "^5.2.3",
    "@angular/material": "^5.0.4",
    "@angular/platform-browser": "^5.2.3",
    "@angular/platform-browser-dynamic": "^5.2.3",
    "@types/node": "^9.3.0",
    "angular-router-loader": "^0.8.2",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.2.5",
    "awesome-typescript-loader": "^3.4.1",
    "clean-webpack-plugin": "^0.1.17",
    "codelyzer": "^4.0.2",
    "copy-webpack-plugin": "^4.3.1",
    "core-js": "^2.5.3",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.8",
    "css-to-string-loader": "^0.1.3",
    "directory-tree-webpack-plugin": "^0.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "postcss-cssnext": "^3.0.2",
    "postcss-flexbugs-fixes": "^3.2.0",
    "postcss-loader": "^2.0.10",
    "pre-build-webpack": "^0.1.0",
    "raw-loader": "^0.5.1",
    "replace": "^0.3.0",
    "resolve-url-loader": "^2.2.1",
    "rxjs": "^5.5.6",
    "sass-loader": "^6.0.6",
    "string-replace-loader": "^1.3.0",
    "style-loader": "^0.19.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "uglifyjs": "^2.4.11",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.0",
    "webpack-dll-bundles-plugin": "^1.0.0-beta.5",
    "webpack-merge": "^4.1.1",
    "zone.js": "^0.8.20",
    "webpack-node-externals": "^1.6.0"
  }
}
